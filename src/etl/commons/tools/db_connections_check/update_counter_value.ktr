    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var max_down_count = getVariable(connection_name + "." + "max.down","-1")
var max_down_count_default = getVariable("default.max.down","-1")

if (max_down_count == -1 &amp;&amp; max_down_count_default != -1) {
	max_down_count=max_down_count_default;
} else {
	max_down_count=10;
}
var counter_value = getVariable(connection_name + "." + "down.value","0")</jsScript_script>
      </jsScript>
